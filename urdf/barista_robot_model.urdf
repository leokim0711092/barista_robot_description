<?xml version = "1.0"?>

<robot name = "cafeteria robots">

    <material name = "red">
        <color rgba = "1 0 0 0.8 "/>
    </material>

    <material name = "white">
        <color rgba = "1 1 1 1 "/>
    </material>
    
    <material name = "blue">
        <color rgba = "0 0 0.9 0.8 "/>
    </material>

    <material name = "black">
        <color rgba = "0.1 0.1 0.1 1 "/>
    </material>


    <link name = "base_link">
    </link>
<!-- Main body -->
    <link name = "main_body">
        <visual>
            <geometry>
                <cylinder radius = "0.178" length = "0.155"/>
            </geometry>
            <material name = "red"/>
        </visual>

        <collision>
            <geometry>
                <cylinder radius = "0.178" length = "0.155"/>
            </geometry>
        </collision>

        <inertial>
            <mass value="8"/>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <inertia ixx="0.079385" ixy="0" ixz="0" iyy="0.079385" iyz="0" izz="0.126736"/>
        </inertial>

    </link>

    <joint name = "base_main_joint" type = "fixed">
        <origin rpy = "0 0 0" xyz = "0 0 0.1127" />
        <parent link = "base_link" />
        <child link = "main_body" />
    </joint>

<!-- left wheel  -->
